function solution(s) {
    var answer = true;
    
   if(!(s.length == 4 || s.length == 6)){
       return false;
   }
    else{
       let s2 = s.split("") ;
        for(var i = 0; i < s2.length; i++){
            if(isNaN(s2[i])){
                return false;
            }
        }
   }
    return answer;
}

============================================
function solution(s) {
    return (s.length === 4 || s.length === 6) && s == parseInt(s)
}
