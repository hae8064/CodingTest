function solution(n) {
    let a = "수박";
    var answer;
    
    if(n % 2 === 0){
        answer = a.repeat(n/2);
    }
    else{
        answer = a.repeat(n/2) + "수";
    }

    return answer;
}


=======================================
function solution(n) {
    return "수박".repeat(n).substring(0,n)
}
