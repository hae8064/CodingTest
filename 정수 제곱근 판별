function solution(n) {   
    var answer = 0;
    
    if(Number.isInteger(Math.sqrt(n))){
        return (Math.sqrt(n) + 1) ** 2;
    }
    else{
        answer = -1;
        return answer;
    }
    // return answer;
}


===========================================
function solution(n) {
    let num = Math.sqrt(n);
    if(num % 1 === 0 ) return (num+1) * (num+1)
    else return -1
}
