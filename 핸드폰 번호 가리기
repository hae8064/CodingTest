function solution(phone_number) {
    var answer = '';
    
    var count = phone_number.length - 4;
    
    var star = "*";
    var resultStar = star.repeat(count);
    
    var backNumber = phone_number.substr(count, 4);
    
    answer = resultStar + backNumber; 
    return answer;
}



======================================다른 풀이===========
function solution(num) {
    const len = num.length - 4
    return "*".repeat(len) + num.substring(len)
}
